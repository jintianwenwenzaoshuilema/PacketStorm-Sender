                /* 全局配置 */
                * { font-family: &quot;JetBrains Mono&quot;, &quot;Consolas&quot;, monospace;
                font-size: 13px; color: #a0a8b7; }
                QMainWindow, QWidget { background-color: #0b0f19; }

                /* GroupBox 样式 */
                QGroupBox { border: 1px solid #1c2333; border-radius: 4px; margin-top: 24px;
                padding-top: 10px; background-color: #0f121a; }
                QGroupBox::title { subcontrol-origin: margin; subcontrol-position: top left; left:
                10px; padding: 0 6px; color: #00f0ff; font-weight: bold; background-color: #0b0f19;
                }

                /* 输入框样式 */
                QLineEdit, QSpinBox, QComboBox, QTextEdit { background-color: #0b0f19; border: 1px
                solid #2d3748; border-radius: 2px; color: #00f0ff; selection-background-color:
                #00f0ff; selection-color: #000000; }
                QLineEdit:focus, QSpinBox:focus, QComboBox:focus, QTextEdit:focus { border: 1px
                solid #00f0ff; background-color: #0b0f19; }
                QLineEdit:disabled, QSpinBox:disabled, QComboBox:disabled { background-color:
                #0b0f19; color: #4a5568; border: 1px dashed #2d3748; }
                QSpinBox::up-button, QSpinBox::down-button { background-color: #0b0f19; border:
                none; }
                /* Interval SpinBox 透明背景 */
                /* Interval SpinBox 透明背景 */
                QSpinBox#spinInterval { background-color: transparent; }
                QSpinBox#spinInterval:focus { background-color: transparent; }
                QSpinBox#spinInterval::up-button, QSpinBox#spinInterval::down-button {
                background-color: transparent; }
                /* Target PPS Label 透明背景 */
                QLabel { background-color: transparent; }

                /* 按钮样式 */
                QPushButton { border: none; border-radius: 2px; padding: 8px; font-weight: bold;
                background-color: rgba(255, 255, 255, 0.05); border: 1px solid #2d3748; }
                QPushButton:hover { background-color: rgba(255, 255, 255, 0.1); border-color:
                #a0a8b7; }
                QPushButton#btnStartSend { background-color: rgba(0, 230, 118, 0.1); border: 1px
                solid #00e676; color: #00e676; padding: 12px; font-size: 14px; }
                QPushButton#btnStartSend:hover { background-color: rgba(0, 230, 118, 0.3); color:
                #ffffff; }
                QPushButton#btnStartSend:disabled { border-color: #2d3748; color: #4a5568;
                background-color: transparent; }
                QPushButton#btnStopSend { background-color: rgba(255, 23, 68, 0.1); border: 1px
                solid #ff1744; color: #ff1744; padding: 12px; font-size: 14px; }
                QPushButton#btnStopSend:hover { background-color: #ff1744; color: #ffffff; }
                QPushButton#btnStopSend:disabled { border-color: #2d3748; color: #4a5568;
                background-color: transparent; }

                /* 复选/单选 */
                QRadioButton, QCheckBox { spacing: 8px; color: #a0a8b7; background-color:
                transparent;
                border: none; padding: 0px; }
                QRadioButton { padding-top: 2px; }
                QRadioButton::indicator, QCheckBox::indicator { width: 14px; height: 14px; border:
                1px solid #666666; background: transparent; }
                QRadioButton::indicator { border-radius: 7px; }
                QRadioButton::indicator:checked, QCheckBox::indicator:checked { border-color:
                #00f0ff; background-color: #00f0ff; }

                /* Splitter 拖动条样式 */
                QSplitter::handle { background-color: #1c2333; }
                QSplitter::handle:hover { background-color: #00f0ff; }
                QSplitter::handle:pressed { background-color: #00e676; }

                /* Splitter 拖动条样式 */
                QSplitter::handle {
                background-color: transparent; /* 平时完全透明 */
                }
                QSplitter::handle:hover {
                background-color: rgba(0, 240, 255, 0.2); /* 鼠标悬停时显示微弱的青色光晕 */
                }
                QSplitter::handle:pressed {
                background-color: rgba(0, 230, 118, 0.5); /* 按下拖动时显示半透明绿色 */
                }
            </string>
