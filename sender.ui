       background-color: #111625;
       border: 1px solid #2d3748;
       border-bottom: 2px solid #2d3748;
       border-radius: 2px;
       color: #00f0ff;
       padding: 6px;
       selection-background-color: #00f0ff;
       selection-color: #000000;
   }

   QLineEdit:focus, QSpinBox:focus, QComboBox:focus {
       background-color: #151a2b;
       border: 1px solid #00f0ff;
       border-bottom: 2px solid #00f0ff;
       color: #ffffff;
   }

/* =======================================================
   RADIO BUTTON (圆形单选框)
   修改策略：
   1. 未选中时：使用 #666666 (亮灰) 边框，确保圆形轮廓清晰可见。
   2. 选中时：边框变为 #00f0ff (青色)，且内部填满。
   ======================================================= */
QRadioButton {
    spacing: 8px;
    color: #a0a8b7;
}

/* --- 未选中状态 (清晰的空心圆环) --- */
QRadioButton::indicator {
    width: 14px;
    height: 14px;

    /* 强制圆形：14的一半是7 */
    border-radius: 7px;

    /* 【修改点】边框改为2px实线，颜色由原来的暗灰改为 #666666，
       这样在深黑背景下就能清楚看到一个圆圈，而不是模糊的一团 */
    border: 2px solid #666666;

    background-color: transparent; /* 背景透明，突出边框 */
}

/* --- 选中状态 (实心发光圆球) --- */
QRadioButton::indicator:checked {
    width: 14px;
    height: 14px;
    border-radius: 7px; /* 保持圆形 */

    /* 【修改点】选中后边框变色 */
    border: 2px solid #00f0ff;

    background-color: #00f0ff; /* 内部填满 */
}

/* 鼠标悬停 */
QRadioButton::indicator:hover {
    border-color: #00f0ff; /* 悬停时边框变亮 */
}


/* =======================================================
   CHECKBOX (方形复选框)
   修改策略：保持微圆角的方形，边框设为 1px 以示区别
   ======================================================= */
QCheckBox {
    spacing: 8px;
    color: #a0a8b7;
}

/* --- 未选中 (空心方框) --- */
QCheckBox::indicator {
    width: 14px;
    height: 14px;

    /* 方形微圆角 */
    border-radius: 2px;

    /* 边框比单选框细一点 (1px)，形成视觉差异 */
    border: 1px solid #666666;

    background-color: transparent;
}

/* --- 选中 (实心方块) --- */
QCheckBox::indicator:checked {
    border-radius: 2px;
    border: 1px solid #00f0ff;
    background-color: #00f0ff;
}

QCheckBox::indicator:hover {
    border-color: #00f0ff;
}

   /* =======================================================
      BUTTONS (按钮)
      ======================================================= */
   QPushButton {
       border: none;
       border-radius: 2px;
       padding: 12px;
       font-weight: bold;
       font-size: 14px;
       letter-spacing: 1px;
   }

   /* --- START 按钮 --- */
   QPushButton#btnStartSend {
       background-color: rgba(0, 230, 118, 0.1);
       border: 1px solid #00e676;
       color: #00e676;
   }
   QPushButton#btnStartSend:hover {
       background-color: rgba(0, 230, 118, 0.3);
       border: 1px solid #ffffff;
       color: #ffffff;
   }
   /* Start 禁用态 (点击后变暗灰) */
   QPushButton#btnStartSend:disabled {
       background-color: transparent;
       border: 1px solid #2d3748;
       color: #4a5568;
   }

   /* --- STOP 按钮 --- */
   QPushButton#btnStopSend {
       background-color: rgba(255, 23, 68, 0.1);
       border: 1px solid #ff1744;
       color: #ff1744;
   }
   QPushButton#btnStopSend:hover {
       background-color: #ff1744;
       color: #ffffff;
   }
   /* Stop 禁用态 */
   QPushButton#btnStopSend:disabled {
       background-color: transparent;
       border: 1px solid #2d3748;
       color: #4a5568;
   }
