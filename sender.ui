}

/* === 单选框 (Radio Button) 样式优化 === */
QRadioButton {
    color: white;            /* 文字颜色：纯白 */
    spacing: 8px;            /* 图标和文字的间距 */
    font-size: 13px;         /* 字号 */
}

/* 指示器（那个圆圈）的基础样式 */
QRadioButton::indicator {
    width: 16px;
    height: 16px;
    border-radius: 9px;      /* 半径设为宽度的一半，使其变成正圆 */
    border: 2px solid #888;  /* 未选中时：灰色边框 */
    background-color: transparent;
}

/* 选中时的样式 */
QRadioButton::indicator:checked {
    background-color: white; /* 选中时：填充白色 */
    border: 2px solid white; /* 边框也变白 */
}

/* 鼠标悬停时的样式 */
QRadioButton::indicator:hover {
    border-color: #DDD;      /* 鼠标放上去时边框变亮 */
}

QPushButton {
        background-color: #333337;
        border: none;
        border-radius: 4px;
        padding: 10px;
        color: white;
        font-weight: bold;
        font-size: 13px;
}

QPushButton:hover {
        background-color: #454548;
}

QPushButton:disabled {
        background-color: #2D2D30;
        color: #555;
}

/* 复选框整体文字颜色 */
QCheckBox {
    color: #FFFFFF;       /* 白色文字 */
    spacing: 5px;         /* 文字和框的间距 */
    font-size: 12px;
}

/* === 指示器（那个方框）的基础样式 === */
QCheckBox::indicator {
    width: 14px;
    height: 14px;

    /* 核心设置：白色边框，2像素宽 */
    border: 2px solid #FFFFFF;

    /* 圆角设置：2px 为微圆角，0px 为直角，根据喜好调整 */
    border-radius: 2px;

    /* 背景透明，只显示白框 */
    background-color: transparent;
}

/* 鼠标悬停时：边框稍微变暗，提供反馈 */
QCheckBox::indicator:hover {
    border-color: #DDDDDD;
}

/* === 选中状态 === */
QCheckBox::indicator:checked {
    /* 选中时：填充为纯白色 (实心白块风格) */
    background-color: #FFFFFF;
    border-color: #FFFFFF;

    /* 如果您有勾选图标资源，可以用 image: url(...)，
       但在扁平化深色主题中，实心白块通常代表“选中”，非常清晰且无需图标 */
    image: none;
}

/* (可选) 半选状态：如果未来用到 Tri-state */
QCheckBox::indicator:indeterminate {
    background-color: #888888;
    border-color: #FFFFFF;
}

/* 特定按钮颜色 */
QPushButton#btnStartSend { background-color: #2E7D32; }
QPushButton#btnStartSend:hover { background-color: #388E3C; }
QPushButton#btnStartSend:disabled { background-color: #1B5E20; color: #666; }

QPushButton#btnStopSend { background-color: #C62828; }
QPushButton#btnStopSend:hover { background-color: #D32F2F; }
QPushButton#btnStopSend:disabled { background-color: #581515; color: #666; }

QPushButton#btnStartRx { background-color: #0069C0; }
QPushButton#btnStartRx:hover { background-color: #2196F3; }
QPushButton#btnStartRx:disabled { background-color: #0D47A1; color: #666; }

QPushButton#btnStopRx { background-color: #C62828; }
QPushButton#btnStopRx:hover { background-color: #D32F2F; }
QPushButton#btnStopRx:disabled { background-color: #581515; color: #666; }

/* 统计大字体 */
QLabel#lblRxCount1 { color: #00E676; font-size: 36px; font-weight: bold; }
QLabel#lblRxCount2 { color: #FFCA28; font-size: 36px; font-weight: bold; }
QLabel#lblDelayVal { color: #29B6F6; font-size: 36px; font-weight: bold; }

QFrame#frameStat1, QFrame#frameStat2, QFrame#frameStat3 {
        background-color: #2D2D30;
        border-radius: 8px;
}
